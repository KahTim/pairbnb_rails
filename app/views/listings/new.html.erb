<h1>Create new listing for <%= current_user.name%></h1>

<%= form_for [current_user, @listing] do |form| %>

	<div>
	  <%= form.label :photos %>
	  <%= form.file_field :photos, multiple: true %>
	</div>
	
	<div>
		<%= form.label :name %>
		<%= form.text_field :name %> 
	</div>

	<div>
	  <%= form.label :place_type %>
	  <%= select_tag :place_type, options_for_select(Listing::PLACE_TYPE_OPTIONS) %>
	</div>

	<div>
	  <%= form.label :property_type %>
	  <%= form.text_field :property_type %> 
	</div>

	<div class="submit-field">
	  <%= form.submit %>
	</div>
<% end %>
<%= link_to "Home", home_index_path%>

